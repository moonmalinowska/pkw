<p id="notice"><%= notice %></p>

<h1>Listing Commitees</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @commitees.each do |commitee| %>
      <tr>
        <td><%= link_to 'Show', commitee %></td>
        <td><%= link_to 'Edit', edit_commitee_path(commitee) %></td>
        <td><%= link_to 'Destroy', commitee, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Commitee', new_commitee_path %>
